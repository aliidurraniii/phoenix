{
  "name": "ili-react-vite-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:folders": "node scripts/lint-folder-names.js",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "prepare": "husky",
    "check:unused": "depcheck",
    "depcheck": "node scripts/check-unused-deps.js",
    "storybook": "storybook dev -p 6006",
    "report:packages": "npm audit --json > packages-reports/npm-audit-report.json || true && license-checker --onlyDirectDependencies --json > packages-reports/packages-license.json && (npm outdated --long > packages-reports/outdated-packages.txt || true)"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.22.0",
    "@emotion/react": "^11.14.0",
    "@tanstack/react-query": "^5.80.10",
    "axios": "^1.10.0",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.555.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-i18next": "^15.5.3",
    "react-router-dom": "^7.6.2",
    "recharts": "^3.5.1"
  },
  "devDependencies": {
    "@cypress/react": "^9.0.1",
    "@cypress/vite-dev-server": "^6.0.3",
    "@eslint/js": "^9.29.0",
    "@storybook/react-vite": "^9.0.18",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-google-recaptcha": "^2.1.9",
    "@vitejs/plugin-react": "^4.4.1",
    "cypress": "^14.5.3",
    "depcheck": "^1.4.7",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-sonarjs": "^3.0.3",
    "eslint-plugin-storybook": "^9.0.18",
    "eslint-plugin-unicorn": "^60.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "license-checker": "^25.0.1",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "storybook": "^9.0.18",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-bundle-obfuscator": "^1.0.2",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "main": "eslint.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
